<yas>
    <scene root="scene" axis_length="5.0" />
    <views default="perspective2">
        <perspective id="perspective" near="0.1" far="1000.0" angle="25.0">
            <from x="300.0" y="300.0" z="300.0" />
            <to x="0.0" y="0.0" z="0.0" />
        </perspective>
        <perspective id="perspective2" near="0.1" far="1000.0" angle="23.0">
            <from x="20.0" y="20.0" z="20.0" />
            <to x="0.0" y="0.0" z="0.0" />
        </perspective>
        <ortho id="ortho" near="0.1" far="1000.0" left="-100.0" right="100.0" top="100.0" bottom="-100.0">
            <from x="300.0" y="300.0" z="300.0" />
            <to x="0.0" y="0.0" z="0.0" />
        </ortho>
    </views>
    <ambient>
        <ambient r="0.3" g="0.3" b="0.3" a="1.0" />
        <background r="0.5" g="0.75" b="0.93" a="1.0" />
    </ambient>
    <lights>
        <omni id="light0" enabled="1">
            <location x="-20.0" y="40.0" z="-20.0" w="1.0" />
            <ambient r="0.1" g="0.1" b="0.1" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="0.0" g="0.0" b="1.0" a="1.0" />
        </omni>
        <omni id="light1" enabled="1">
            <location x="-20.0" y="40.0" z="20.0" w="1.0" />
            <ambient r="0.1" g="0.1" b="0.1" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="0.0" g="0.0" b="1.0" a="1.0" />
        </omni>
        <omni id="light2" enabled="1">
            <location x="20.0" y="40.0" z="20.0" w="1.0" />
            <ambient r="0.1" g="0.1" b="0.1" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="0.0" g="0.0" b="1.0" a="1.0" />
        </omni>
        <omni id="light3" enabled="1">
            <location x="20.0" y="40.0" z="-20.0" w="1.0" />
            <ambient r="0.1" g="0.1" b="0.1" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="0.0" g="0.0" b="1.0" a="1.0" />
        </omni>
        <spot id="light4" enabled="1" angle="45.0" exponent="1.0">
            <location x="-190.0" y="20.0" z="30.0" w="1.0"/>
            <target x="-200.0" y="20.0" z="0.0" />
            <ambient r="0.1" g="0.1" b="0.1" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="0.0" g="0.0" b="1.0" a="1.0" />
        </spot>
        <spot id="light5" enabled="1" angle="45.0" exponent="1.0">
            <location x="-190.0" y="20.0" z="-30.0" w="1.0"/>
            <target x="-200.0" y="20.0" z="0.0" />
            <ambient r="0.1" g="0.1" b="0.1" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </spot>
    </lights>
    <textures>
        <texture id="terrain" file="scenes/images/terrain_texture.jpg" />
        <texture id="heightmap" file="scenes/images/heightmap.jpg" />
        <texture id="water" file="scenes/images/water_texture.jpg" />
        <texture id="wavemap" file="scenes/images/wavemap.jpg" />
    </textures>
    <materials>
        <material id="default" shininess = "10.0">
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
            <diffuse r="0.5" g="0.5" b="0.5" a="1.0" />
            <specular r="0.5" g="0.5" b="0.5" a="1.0" />
        </material>
    </materials>
    <transformations>
        <transformation id="vehicle">
            <translate x="-10.0" y="0.0" z="0.0" />
        </transformation>
    </transformations>
    <animations>
        <linear id="vehicle_start" span="5">
            <controlpoint xx="-10.0" yy="0.0" zz="0.0" />
            <controlpoint xx="0.0" yy="0.0" zz="-10.0" />
            <controlpoint xx="10.0" yy="10.0" zz="-10.0" />
            <controlpoint xx="20.0" yy="20.0" zz="-15.0" />
            <controlpoint xx="25.0" yy="20.0" zz="-15.0" />
        </linear>
        <circular id="vehicle_mid" span="5" center="25.0 20.0 0.0" radius="15" startang="180" rotang="-180" />
        <linear id="vehicle_end" span="5">
            <controlpoint xx="25.0" yy="20.0" zz="15.0" />
            <controlpoint xx="20.0" yy="20.0" zz="15.0" />
            <controlpoint xx="10.0" yy="10.0" zz="10.0" />
            <controlpoint xx="0.0" yy="0.0" zz="10.0" />
            <controlpoint xx="-10.0" yy="0.0" zz="0.0" />
        </linear>
    </animations>
    <primitives>
        <primitive id="plane">
            <plane npartsU="20" npartsV="20" />
        </primitive>
        <primitive id="vehicle">
            <vehicle/>
        </primitive>
        <primitive id="vehicle2">
            <vehicle/>
        </primitive>
        <primitive id="patch">
            <patch npointsU="3" npointsV="2" npartsU="20" npartsV="20">
                <controlpoint xx="-1.5" yy="-1.5" zz="0.0" />
                <controlpoint xx="-1.5" yy="1.5" zz="0.0" />
                <controlpoint xx="0" yy="-1.5" zz="3.0" />
                <controlpoint xx="0" yy="1.5" zz="3.0" />
                <controlpoint xx="1.5" yy="-1.5" zz="0.0" />
                <controlpoint xx="1.5" yy="1.5" zz="0.0" />
            </patch>
        </primitive>

        <primitive id="cylinder2">
            <cylinder2 base="0.5" top="1.0" height="1.0" slices="5" stacks="5" />
        </primitive >

        <primitive id="cylinder">
            <cylinder base="0.5" top="1.0" height="1.0" slices="5" stacks="10" />
        </primitive>
        <primitive id="terrain">
            <terrain idtexture="terrain" idheightmap="heightmap" parts="20" heightscale="100"/>
        </primitive>
        <primitive id="water">
            <water idtexture="water" idwavemap="wavemap" parts="20" heightscale="100" texscale="100"/>
        </primitive>
    </primitives>
    <components>
        <component id="scene">
            <transformation>
            </transformation>
            <materials>
                <material id="default" />
            </materials>
            <texture id="none" />
            <children>
                <!-- <componentref id="patch"/> -->
                <!--<componentref id="plane"/>-->
                <componentref id="cylinder2"/> -->
                <!-- <componentref id="cylinder"/> -->
                <!-- <componentref id="vehicle"/> -->
                <!--<componentref id="terrain"/>
                <componentref id="water"/>-->
                <!--<componentref id="vehicle2"/>-->
            </children>
        </component>
        <component id="terrain">
            <transformation>
                <scale x="100" y="1" z="100"/>
                <translate x="0" y="5" z="0"/>
            </transformation>
            <materials>
                <material id="default" />
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="terrain"/>
            </children>
        </component>
        <component id="water">
            <transformation>
                <scale x="200" y="1" z="200"/>
            </transformation>
            <materials>
                <material id="default" />
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="water"/>
            </children>
        </component>
        <component id="vehicle">
            <transformation>
                <transformationref id="vehicle"/>
            </transformation>
            <animations>
                <animationref id="vehicle_start"/>
                <animationref id="vehicle_mid"/>
                <animationref id="vehicle_end"/>
            </animations>
            <materials>
                <material id="default" />
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="vehicle"/>
            </children>
        </component>

        <component id="vehicle2">
            <transformation>
                <transformationref id="vehicle"/>
            </transformation>
            <materials>
                <material id="default" />
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="vehicle2"/>
            </children>
        </component>

        <component id="patch">
            <transformation>
                <translate x="5" y="1" z="1"/>
            </transformation>
            <materials>
                <material id="default" />
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="patch"/>
            </children>
        </component>

        <component id="cylinder2">
            <transformation>
            </transformation>
            <materials>
                <material id="default" />
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="cylinder2"/>
            </children>
        </component>

        <component id="cylinder">
            <transformation>
                <translate x="5" y="0" z="0"/>
            </transformation>
            <materials>
                <material id="default" />
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="cylinder"/>
            </children>
        </component>

        <component id="plane">
            <transformation>
                <scale x="1" y="1" z="1"/>
            </transformation>
            <materials>
                <material id="default" />
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="plane"/>
            </children>
        </component>
    </components>
</yas>